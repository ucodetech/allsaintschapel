RewriteRule ^screening-detail/([0-9a-zA-Z_-]+)$ screening-detail.php?detail=$1 [NC,L]
RewriteRule ^counsel-detail/([0-9a-zA-Z_-]+)$ counsel-detail.php?detail=$1 [NC,L]
RewriteRule ^sermon-text/([0-9a-zA-Z_-]+)$ sermon-text.php?detail=$1 [NC,L]
RewriteRule ^sermon-audio/([0-9a-zA-Z_-]+)$ sermon-audio.php?detail=$1 [NC,L]
RewriteRule ^bulletin-detail/([0-9a-zA-Z_-]+)$ bulletin-detail.php?detail=$1 [NC,L]
RewriteRule ^bulletin-edit/([0-9a-zA-Z_-]+)$ bulletin-edit.php?edit=$1 [NC,L]
